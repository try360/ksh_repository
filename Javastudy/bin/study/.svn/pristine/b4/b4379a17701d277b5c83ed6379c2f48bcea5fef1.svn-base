}
.writeTable td {
    padding: 5px 10px;
    border-bottom: 1px solid #DDDDDD;
}
.writeTable tr:last-child th {
    border-bottom: 0;
}
.writeTable tr:last-child td {
    border-bottom: 0;
}
.writeTable td input[type=text], .writeTable td input[type=number] {
    width: calc(100% - 10px);
    padding: 3px 5px;
}
.writeTable td textarea {
    width: calc(100% - 10px);
    height: 300px;
    padding: 3px 5px;
}


