.contents_form {
    display: inline-block;
    clear: both;
    padding-top:  25px;
    width: 100%;
}

.divDocTitle {
    margin: 0 25px 10px 25px;
    padding-bottom: 15px;
    border-bottom: 1px solid #d2d2d2;
    font-size: 12pt;
    font-weight: bolder;
    color: #333333;
}
.searchArea {
    margin: 0 25px;
    padding-bottom: 10px;
    float: left;
}
.btnArea {
    float: right;
    margin-right: 25px;
    margin-bottom: 10px;
}
.leftBtnArea {
    float: left;
    margin-left: 25px;
}
.listTable {
    clear: both;
    table-layout: fixed;
    display: table;
    margin: 0 25px 10px 25px;
    width: calc(100% - 50px);
    border-top: 2px solid rgba(102, 51, 102, 0.54);
}
.listTable tbody tr:not(.notHover):hover {
	background-color: #ECECEC;
}
.listTable th {
    color: #5f115d;
    background: rgba(95, 17, 93, 0.13);
    font-weight: bold;
    height: 28px;
    text-align: center;
    padding-top: 3px;
    border-right: 1px solid rgba(102, 51, 102, 0.54);
    border-bottom: 1px solid rgba(102, 51, 102, 0.54);
}
.listTable th:last-child {
    border-right: 0;
}
.listTable td {
    height: 25px;
    text-align: center;
    border-right: 1px solid #DDDDDD;
    border-bottom: 1px solid #DDDDDD;
    padding: 3px 0;
    font-family: dotum, "Sans-Serif";
    color: #666;
    font-size: 9pt;
}
.listTable td:last-child {
    border-right: 0;
}
.listTable td.left {
    text-align: left;
    padding-left: 10px;
}
.listTable td a:hover {
    text-decoration: underline;
}
.listTable td span.title {
    max-width: calc(100% - 60px);
    display: inline-block;
    vertical-align: middle;
    white-space: nowrap;
    overflow: hidden;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
}
.listTable td em {
    font-family: Tahoma;
    font-size: 8pt;
    color: hotpink;
    font-style: normal;
    margin-right: 3px;
    font-weight: bold;
}
.writeTable {
    clear: both;
    table-layout: fixed;
    display: table;
    margin: 0 25px 10px 25px;
    width: calc(100% - 50px);
    border-top: 2px solid rgba(102, 51, 102, 0.54);
    border-bottom: 1px solid rgba(102, 51, 102, 0.54);
}
.writeTable th {
    color: #5f115d;
    background: rgba(95, 17, 93, 0.13);
    font-weight: bold;
    height: 28px;
