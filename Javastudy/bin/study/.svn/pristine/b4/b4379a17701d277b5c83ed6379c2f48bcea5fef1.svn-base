/* paging */
.paging {
    width: 100%;
    text-align: center;
    vertical-align: middle;
}
.paging ul {

}
.paging ul li {
    display: inline-block;
 
    font-family: dotum, gulim, utum;
    font-size: 9pt;
    vertical-align: middle;
    border: 1px solid #CCCCCC;
    border-right: 0;
    width: 36px;
 	padding: 10px 0;
 	marging: 0;
 	color: #ff0000;
 	font-weight: bold;
 	margin-left: -5px;
}
.paging ul a li {
    color: #777777;
    font-weight: normal;
}
.paging ul li .ui-icon {
	background-image: url('/resources/js/jquery-ui-1.12.1.custom/images/ui-icons_777777_256x240.png');
}
.paging ul a:hover li {
    font-weight: bolder;
    color: #000000;
}
.paging ul a:last-child li {
	border-right: 1px solid #CCCCCC;
	border-radius: 0 10px 10px 0;
}
.paging ul a:first-child li {
	border-radius: 10px 0 0 10px; 
}

.replyArea {
    clear:both;
    width:calc(100% - 40px);
    background:#ECECEC;
    margin: 0 20px 0 20px !important;
    position: relative;
    padding-bottom: 10px;
}

.replyTable {
    clear: both;
    table-layout: fixed;
    display: table;
    margin: 0 20px 0  20px;
    width: calc(100% - 50px);
    padding-top: 20px;
}
.replyTable td {
    padding: 5px 0;
    text-align: right;
}
.replyTable td textarea {
    width: calc(100% - 12px);
    height: 42px;
    padding: 3px 5px;
}
.replyArea dl {
    display: block;
    margin: 0 20px;
    padding: 10px 0;
    width: calc(100% - 40px);
    margin:0 auto;
    border-bottom: 1px dotted #CCCCCC;
}
.replyArea dl dd {
    display: block;
    margin: 0;
    padding: 0;
    font: 10pt Gulim;
    letter-spacing: 1px;
}
.replyArea dl dd:first-child {
    margin-bottom: 5px;
    font-size: 8pt;
    font-family: Arial;
    color: #666666;
}
.replyArea dl dd:first-child strong {
    font-family: Gulim;
    font-size: 9pt;
    font-weight: bolder;
    color: #000000;
}

.ui-datepicker-month {
    width: auto;
    vertical-align: middle;
    font-family: Tahoma !important;
    font-size: 10pt !important;
}
.ui-datepicker-year {
    width: auto;
    vertical-align: middle;
    font-family: Tahoma !important;
    font-size: 10pt !important;
}
.datepicker {
    padding : 7px 5px !important;
    font-size: 9pt;
    font-family: tahoma;
    -webkit-border-radius: 3px 0 0 3px;
    -moz-border-radius: 3px 0 0 3px;
    border-radius: 3px 0 0 3px;
    border: 1px solid #CCC;
    border-right: 0;
    margin-bottom: 0px !important;
    width: 80px !important;
}

