@charset "EUC-KR";

body                { margin:0; padding:0; font-size:10pt; font-family:dotum, gulim, utum;}
a                   { text-decoration: none; color: inherit;}
a:hover, a:visited  { text-decoration: none; }
div                 { text-align:left; margin:0; padding:0; }
ul                  { margin:0; padding:0; list-style:none; }
li                  { margin:0; padding:0; }
input               { vertical-align: middle; }
input[type=image]   { margin:0; padding:0; vertical-align: middle; }
img                 { vertical-align: middle;}
select, option      { vertical-align: middle; font-size: 9pt; background: #ffffff; color: #666666; font-family: dotum, gulim, utum; padding: 4px 0;}
.txtarea            { border: 1px solid #BBBBBB; color: #666666; height: 20px; font-size: 9pt; font-family: dotum, gulim, utum; padding-left: 4px; }
fieldset            { border:0; margin: 0; padding: 0;}
legend              { display:none; }
.w180               { width: 180px !important;}
.w30                { width: 30px !important;}
.right				{ text-align: right !important; margin-right: 15px;}
.center				{ text-align: center !important;}
form, fieldset		{ clear:both; margin: 0; padding: 0; }
.left				{ text-align: left !important;}
.h100				{ height: 100px !important;}


/* header Start */
header {
    width:100%;
    min-width: 1024px;
    height:22px;
    background:#FFF;
    border-bottom: 1px solid #D2D2D2;
}
header ul {
    display:block;
    float:right;
}
header ul li {
    max-height: 22px;
    padding: 4.5px 8px;
    float: left;
    text-align: center;
    font-size: 11px;
    font-weight: bold;
    color: #7C7C7C;
    background-color: #FFFFFF;
    border-right: 1px solid #D2D2D2;
}
header ul li a {
    color: #7C7C7C;
}
header ul li.userInfo {
    background-color: #E9E9E9;
    border-left: 1px solid #D2D2D2;
}
header ul li span.ft_black {
    color: #000000;
}
header ul li.important {
	background-color: #D2D2D2;
}
header ul li.important a {
	color: #ff0000;
	font-weight: bold; 
}

/* top Navi Start */
nav {
    display: block;
    width: 100%;
    min-width: 1024px;
    height: 107px;
    top: 24px;
    clear: both;
}
nav ul.top_menu {
    display: block;
    width: 100%;
    min-width: 1024px;
    float: left;
    list-style-type: none;
    border-bottom: 1px solid #D2D2D2;
}
nav ul.top_menu li {
    display: inline;
    padding: 19px 35px;
    float: left;
    text-align: center;
    font-size: 14pt;
    font-weight: bolder;
    font-family: "Arial Black";
    letter-spacing: -1.5px;
    color: #cb14c9;
}
nav ul.top_menu li.l_logo {
    display: inline-block;
    width: 100px;
    height: 65px;
    padding: 0 15px;
}
nav ul.top_menu li a {
    color: #3D3D3D;
}
nav ul.top_menu li a:hover {
    color: #cb14c9;
}
nav ul.top_menu li.active a {
	color: #cb14c9;
}
nav div.sub_menu {
    clear: both;
    display: block;
    width: 100%;
    min-width: 1024px;
    height: 40px;
    background-color: #E9E9E9;
    border-bottom: 1px solid #D2D2D2;
}
nav div.sub_menu ul {
    display: none;
    position: relative;
    width: 980px;
    margin-left: 150px;
    top: 11px;
}
nav div.sub_menu ul li {
    display: inline;
    min-width: 50px;
    padding: 12px 9px;
    font-size: 10pt;
    font-weight: bold;
    letter-spacing: -0.5pt;
    color: #999999;
}

nav div.sub_menu ul li a {
	color: #000000;
}
nav div.sub_menu ul li a:hover {
    color: #cb14c9;
}

/* left Navi Start */
aside {
    position: absolute;
    clear: both;
    background: #FFFFFF;
    padding:0;
    width: 200px;
    min-height: 600px;
    height: auto !important;
    height: 600px;
    float: left;
    display: block;
}
aside ul {

}
aside h3 {
    color: #000000;
    font-size: 18pt;
    font-weight: bolder;
    cursor: default;
    font-family: "Arial Black";
    padding: 48px 0 13px 10px;
    margin: 0;
}
aside ul li {
    height: 16px;
    padding: 9px 15px;
    font-family: ∏º¿∫∞ÌµÒ, ∞ÌµÒ;
    font-size: 11pt;
    font-weight: bolder;
    background-color: #E9E9E9;
    border-bottom: 3px solid #FFFFFF;
    color: #999999;
}
aside ul li a {
	color: #000000;
}
aside ul li.active {
    color: #ffffff;
    background-color: #5f115d;
}
aside ul li.active a {
	color:#ffffff;	
}
aside span.lv2Group {
	display:none;
}
aside ul li.lv2 {
	padding: 6px 20px;
	font-size: 9pt;
	background-color: #FFF;
	border-bottom: 1px solid #E9E9E9;
	display: list-item;
	vertical-align: middle;
}
aside ul li.lv2Active {
	padding: 6px 20px;
	font-size: 9pt;
	background-color: #DDD;
	border-bottom: 1px solid #E9E9E9;
	display: list-item;
	vertical-align: middle;
	color: #5f115d !important;
	font-weight: bold; 
}
/* ∫ªπÆ Ω√¿€ */
section {
    width: calc(100% - 200px);
    margin-left: 200px;
    z-index: 1;
    min-height: 600px;
    height: auto !important;
    height: 600px;
    float: left;
    margin-bottom: 30px;
}
section h1 {
    height: 39px;
    margin: 0;
    font-size: 18pt;
    font-weight: bolder;
    letter-spacing: -1pt;
    padding: 55px 0 0  30px;
    color: #ffffff;
    background-color: #5f115d;
    background-repeat: no-repeat;
}
section h1.rate {
    background-image: url('../img/top_rate.png');
}
section h1.setting {
    background-image: url('../img/top_setting.png');
}
section h1.list {
    background-image: url('../img/top_list.png');
}
section h1.folder {
    background-image: url('../img/top_folder.png');
}
section h1.req {
    background-image: url('../img/top_req.png');
}
section h1.msg {
    background-image: url('../img/top_msg.png');
}
section h1.chk {
    background-image: url('../img/top_chk.png');
}
/* footer start */
footer {
    clear: both;
    width: 100%;
    font-size: 9pt;
    color: #565656;
    text-align: center;
    background-color: #c4c4c4;
    padding: 20px 0;
}

footer div {
	clear: both;
	padding-top: 10px;
	margin: 0 aut;
	text-align: center;
	letter-spacing: 0.5pt;
}
