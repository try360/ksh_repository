.ui-icon {
    vertical-align: text-bottom;
}

.writeArea {
    clear: both;
    margin: 10px 20px 0 20px;
    background: #e9e9e9;
    padding: 20px 20px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
.writeArea select {
    padding: 5px 5px;
}
.writeArea input[type=text] {
	padding: 3px 5px;
	width: 120px;
}
.writeArea input[type=number] {
	padding: 3px 5px;
	width: 80px;
}

#btnRegist {
}

.dragHandle {
    cursor: move;
}

.tDnD_whileDrag {
    background: #E9E9E9;
    -webkit-box-shadow: 6px 3px 5px #555, 0 1px 0 #ccc inset, 0 -1px 0 #ccc inset;
}

.bottomArea {
	width:100%;
	display: inline-block;
	clear:both;
}

.listTable tr.notHover td {
	background-color: #CECECE;
	padding: 2px 3px;
}

.listTable tr.notHover input[type=number], .listTable tr.notHover input[type=text] {
	font-family: sans-serif;
	font-size: 9pt;
	text-align: center;
	width: calc(100% - 6px);
}

button.iconBtn {
	background-color: #f6f6f6;
	border: 1px solid #c5c5c5;
	border-radius: 3px;
	padding: 2px 5px 3px 5px;
	vertical-align: middle;
	cursor: pointer;
}

.inTable {
	margin-top: 0;
	border-top: 1px solid #DDDDDD;
}

#textArea{
	overflow: auto scroll;
	height: 300px;
	border: 1px solid #aaaaaa;
}

input[name^="cost"] {
	text-align: right !important;
} 

#controlgroup input[type='text'] {
	float: left;
	font-family: malgunGothic, Verdana;
	font-size: 10pt;
	height: 29px;
	padding: 0 10px;
	border-left: 0;
	border-right: 0;
	border-top: 1px solid #C5C5C5;
	border-bottom: 1px solid #C5C5C5;
	vertical-align: bottom;
}

/* ¸¶½ºÅ© ¶Ù¿ì±â */
#mask {  
    position:absolute;  
    z-index:9000;  
    background-color:#000;  
    display:none;  
    left:0;
    top:0;
} 
/* ÆË¾÷À¸·Î ¶ß´Â À©µµ¿ì css  */ 
.window{
    display: none;
    position:absolute;  
    left:50%;
    top:150px;
    /* margin-left: -340px; */
    width:710px;
    height:550px;
    background-color:#FFF;
    z-index:10000;   
 }

/*#IE 10+*/
@media screen and(-ms-high-contrast: active), (-ms-high-contrast: none) {
	#controlgroup input[type='text'] {
		height: 27px;
	}
}

.search_box{
	padding-top: 10px;
}

.search_box .search_span{
	width: 100px;
}
