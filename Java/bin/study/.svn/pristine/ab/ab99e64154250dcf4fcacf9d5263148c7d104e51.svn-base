#breadcrumb {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#breadcrumb {
  margin-top: 30px;
  width: 100%;
  /*background-color: #f2f2f2;*/
  position: relative;
  margin-left: 20px;
}

#breadcrumb ul {
  margin: 0;
  padding: 0;
  position: relative;
  top: -20px;
}

#breadcrumb li {
  display: block;
  /*width: 20px;
  height: 20px;*/
  float: left;
  background-color: #fff;
  border-radius: 10px;
  font-family: "Helvetica Neue", sans-serif;
  font-weight: 200;
  font-size: 12px;
  text-align: center;
  position: relative;
  margin: 0 10px;
  line-height: 4.5;
  color: #bfbfbf;
}
#breadcrumb li:before {
  content: "";
  display: block;
  width: 8px;
  height: 8px;
  background: #bfbfbf;
  border-radius: 4px;
  position: absolute;
  top: 6px;
  left: 6px;
  z-index: 10;
}

#breadcrumb .active {
  color: #5f115d;
}
#breadcrumb .active:before {
  content: "";
  display: block;
  width: 10px;
  height: 10px;
  background: #5f115d;
  border-radius: 10px;
  position: absolute;
  top: 5px;
  left: 5px;
  z-index: 10;
}

#breadcrumb .line {
  display: block;
  background: #fff;
  height: 10px;
  border-radius: 5px;
  z-index: 3;
  position: relative;
  top: -5px;
}
#breadcrumb .line:before {
  content: "";
  border-bottom: 1px solid #bfbfbf;
  height: 0;
  width: 90%;
  position: absolute;
  top: 5px;
  left: 5px;
  z-index: 5;
  padding: 0 20px;
}

